<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="goodsBatis">
	<insert id="uploadGoods" parameterType="com.acorn.cherryM1.goodsDto.goodsDto">
		insert into goods 
		(num,price,title,regdate,issaled,category,viewcount,imagepath,explain) 
		values(goods_seq.nextval,#{price},#{title},SYSDATE,0,#{category},0,#{imagePath},#{explain})
	</insert>
</mapper>